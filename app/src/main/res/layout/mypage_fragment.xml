<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Birth_title"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?android:windowBackground">

    <include
        android:id="@+id/common_header"
        layout="@layout/common_header" />


    <LinearLayout
        android:id="@+id/mypage_frame1"
        android:layout_width="345dp"
        android:layout_height="405dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/mypage_frame_1"
        android:fontFamily="@font/omyu_pretty"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/mypage_nationality_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="35dp"
            android:fontFamily="@font/omyu_pretty"
            android:text="대한민국 REPUBLIC OF KOREA"
            android:textColor="@color/black"
            android:textSize="26sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:layout_width="290dp"
            android:layout_height="224dp"
            android:layout_marginTop="26dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/mypage_profile_image"
                android:layout_width="133dp"
                android:layout_height="224dp"
                android:src="@drawable/mypage_profile"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mypage_nationality_text" />

            <LinearLayout
                android:layout_width="136dp"
                android:layout_height="224dp"
                android:layout_marginLeft="21dp"
                android:orientation="vertical">


                <TextView
                    android:id="@+id/surename_title"
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="성 / Surname"
                    android:textColor="@color/unselected"
                    android:textSize="13sp"
                    app:layout_constraintStart_toEndOf="@+id/mypage_profile_image"
                    app:layout_constraintTop_toTopOf="@+id/mypage_profile_image" />

                <TextView
                    android:id="@+id/surname_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="Surname"
                    android:textColor="@color/primary_700"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="@+id/surename_title"
                    app:layout_constraintTop_toBottomOf="@+id/surename_title" />

                <TextView
                    android:id="@+id/first_name_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="11dp"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="이름 / First name"
                    android:textColor="@color/unselected"
                    android:textSize="13sp"
                    app:layout_constraintStart_toEndOf="@+id/mypage_profile_image"
                    app:layout_constraintTop_toBottomOf="@+id/surname_text" />

                <TextView
                    android:id="@+id/first_name_text"
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="First name"
                    android:textColor="@color/primary_700"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="@+id/first_name_title"
                    app:layout_constraintTop_toBottomOf="@+id/first_name_title" />

                <TextView
                    android:id="@+id/korean_name_title"
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:layout_marginTop="11dp"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="한글성명"
                    android:textColor="@color/unselected"
                    android:textSize="13sp"
                    app:layout_constraintStart_toEndOf="@+id/mypage_profile_image"
                    app:layout_constraintTop_toBottomOf="@+id/first_name_text" />

                <TextView
                    android:id="@+id/korean_name_text"
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="한글 성명"
                    android:textColor="@color/primary_700"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="@+id/korean_name_title"
                    app:layout_constraintTop_toBottomOf="@+id/korean_name_title" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:layout_marginTop="11dp"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="생년 월일 / Date of Birth"
                    android:textColor="@color/unselected"
                    android:textSize="13sp"
                    app:layout_constraintStart_toEndOf="@+id/mypage_profile_image"
                    app:layout_constraintTop_toBottomOf="@+id/korean_name_text" />

                <TextView
                    android:id="@+id/birth_text"
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="00 0월 / XXX 2000"
                    android:textColor="@color/primary_700"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="@+id/textView8"
                    app:layout_constraintTop_toBottomOf="@+id/textView8" />

                <TextView
                    android:id="@+id/nationality_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="11dp"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="국적 / Nationality"
                    android:textColor="@color/unselected"
                    android:textSize="13sp"
                    app:layout_constraintStart_toEndOf="@+id/mypage_profile_image"
                    app:layout_constraintTop_toBottomOf="@+id/birth_text" />

                <TextView
                    android:id="@+id/nationality_text"
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="REPUBLIC OF KOREA"
                    android:textColor="@color/primary_700"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="@+id/nationality_title"
                    app:layout_constraintTop_toBottomOf="@+id/nationality_title" />

            </LinearLayout>

        </LinearLayout>

        <TextView
            android:id="@+id/passport_text_line_1"
            android:layout_width="310dp"
            android:layout_height="18dp"
            android:layout_marginTop="26dp"
            android:gravity="center"
            android:text="Surname&lt;&lt;Firstname&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;&lt;"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mypage_profile_image" />


        <TextView
            android:id="@+id/passport_text_line_2"
            android:layout_width="310dp"
            android:layout_height="18dp"
            android:fontFamily="@font/omyu_pretty"
            android:gravity="center"
            android:text="000000XXX00000000000000000000000ME-MORY"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mypage_profile_image" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/mypage_frame2"
        android:layout_width="345dp"
        android:layout_height="150dp"
        android:layout_marginTop="500dp"
        android:background="@drawable/mypage_frame_1"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/country_diary_layout"
            android:layout_width="138dp"
            android:layout_height="53dp"
            android:layout_marginTop="20dp">


            <LinearLayout
                android:id="@+id/diary_count_layout"
                android:layout_width="wrap_content"
                android:layout_height="51dp"
                android:gravity="center"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/diary_count"
                    android:layout_width="wrap_content"
                    android:layout_height="21dp"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="00"
                    android:textColor="@color/primary_700"
                    android:textSize="25sp" />

                <TextView
                    android:id="@+id/diary_count_title"
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:layout_marginTop="15dp"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="일기"
                    android:textColor="@color/primary_700"
                    android:textSize="16sp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/visited_country_count_layer"
                android:layout_width="65dp"
                android:layout_height="51dp"
                android:gravity="center"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/visited_country_count"
                    android:layout_width="wrap_content"
                    android:layout_height="21dp"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="00"
                    android:textColor="@color/primary_700"
                    android:textSize="25sp" />

                <TextView
                    android:id="@+id/visited_country_title"
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:layout_marginTop="15dp"
                    android:fontFamily="@font/omyu_pretty"
                    android:text="방문한 나라"
                    android:textColor="@color/primary_700"
                    android:textSize="16sp" />

            </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/country_flag_text_1"
            android:layout_width="297dp"
            android:layout_height="18dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="10dp"
            android:gravity="center"
            android:text="---"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mypage_profile_image" />


        <TextView
            android:id="@+id/country_flag_text_2"
            android:layout_width="297dp"
            android:layout_height="18dp"
            android:gravity="center"
            android:text="---"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mypage_profile_image" />


    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>